# A sample Gemfile
source "https://rubygems.org"

gem "jekyll", ">= 3.6.3"
gem "jekyll-redirect-from"

# A fork of the built-in Jekyll Sass converter that depends on libsass via the
# sassc gem, as Ruby Sass (the sass gem) reached EOL in March 2019.
gem "jekyll-sass-converter", :git =>
  "https://github.com/searls/jekyll-sassc-converter.git"
